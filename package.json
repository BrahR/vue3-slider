{
  "name": "vue3-slider",
  "version": "1.0.2",
  "author": "Freddie Nelson <freddie0208@hotmail.com> (https://freddienelson.co.uk)",
  "description": "A modern, customizable slider built entirely in Vue.js 3 with TypeScript.",
  "homepage": "https://github.com/freddie-nelson/vue3-slider",
  "repository": {
    "type": "git",
    "url": "https://github.com/freddie-nelson/vue3-slider"
  },
  "bugs": {
    "url": "https://github.com/freddie-nelson/vue3-slider/issues",
    "email": "freddie0208@hotmail.com"
  },
  "license": "MIT",
  "private": false,
  "main": "dist/vue3-slider.ssr.js",
  "browser": "dist/vue3-slider.esm.js",
  "module": "dist/vue3-slider.esm.js",
  "unpkg": "dist/vue3-slider.min.js",
  "types": "vue3-slider.d.ts",
  "files": [
    "dist/*",
    "vue3-slider.d.ts",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "build": "yarn build:ssr && yarn build:es && yarn build:unpkg",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "build:demo": "vue-cli-service build demo/demo.ts --dest docs/"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@babel/preset-env": "^7.12.0",
    "@babel/preset-typescript": "^7.12.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/resize-observer-browser": "^0.1.4",
    "@vue/cli-plugin-babel": "^4.5.7",
    "@vue/cli-plugin-typescript": "^4.5.7",
    "@vue/cli-service": "^4.5.7",
    "@vue/compiler-sfc": "^3.0.0",
    "cross-env": "^7.0.2",
    "minimist": "^1.2.5",
    "node-sass": "^5.0.0",
    "rollup": "^2.30.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-vue": "^6.0.0-beta.10",
    "sass-loader": "^10.0.5",
    "typescript": "^3.8.3",
    "vue": "^3.0.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "engines": {
    "node": ">=10"
  }
}
